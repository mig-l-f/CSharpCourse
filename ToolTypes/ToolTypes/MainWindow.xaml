<Window x:Class="ToolTypes.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
        <Window.Resources>
            <Style TargetType="Button">
                <Setter Property="Margin" Value="0,5" />
                <Setter Property="FontSize" Value="24" />
        </Style>
        </Window.Resources>
        <StackPanel x:Name="mainTextBlock">
    <TextBlock x:Name="tbOutput" Height="48" HorizontalAlignment="Center" FontSize="24" />
        <Button Content="time" Click="time_Click"/>
        <Button Content="types" Click="types_Click"/>
        <Button Content="types (array)" Click="types_a_Click"/>
        <Button Content="types (slow)" Click="types_d_Click"/>
        <Button Content="types (slow but async)"  Click="types_sa_Click"/>
    </StackPanel >
</Window>
